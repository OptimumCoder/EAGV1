{
    "manifest_version": 3,
    "name": "Text Extractor & Search",
    "version": "1.0",
    "description": "Extracts text from web pages and searches extracted content.",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
  }